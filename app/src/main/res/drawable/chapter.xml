<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="8dp" />
            <gradient
                android:angle="270"
                android:startColor="#2A4B3A"
                android:endColor="#1C3528"
                android:type="linear" />
            <stroke
                android:width="1dp"
                android:color="#33FFFFFF" />
        </shape>
    </item>

    <item
        android:width="28dp"
        android:height="28dp"
        android:gravity="top|start"
        android:top="4dp"
        android:start="4dp">
        <vector
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:pivotX="12"
                android:pivotY="12"
                android:rotation="0">
                <path
                    android:pathData="M3,21 C8,18 10,12 10,10 C10,8 12,5 18,3"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round"
                    android:strokeWidth="1.5"
                    android:strokeColor="#A0D4E1D5" />
            </group>
        </vector>
    </item>

    <item
        android:width="28dp"
        android:height="28dp"
        android:gravity="top|end"
        android:top="4dp"
        android:end="4dp">
        <vector
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:pivotX="12"
                android:pivotY="12"
                android:rotation="90">
                <path
                    android:pathData="M3,21 C8,18 10,12 10,10 C10,8 12,5 18,3"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round"
                    android:strokeWidth="1.5"
                    android:strokeColor="#A0D4E1D5" />
            </group>
        </vector>
    </item>

    <item
        android:width="28dp"
        android:height="28dp"
        android:gravity="bottom|end"
        android:bottom="4dp"
        android:end="4dp">
        <vector
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:pivotX="12"
                android:pivotY="12"
                android:rotation="180">
                <path
                    android:pathData="M3,21 C8,18 10,12 10,10 C10,8 12,5 18,3"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round"
                    android:strokeWidth="1.5"
                    android:strokeColor="#A0D4E1D5" />
            </group>
        </vector>
    </item>
    
    <item
        android:width="28dp"
        android:height="28dp"
        android:gravity="bottom|start"
        android:bottom="4dp"
        android:start="4dp">
        <vector
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:pivotX="12"
                android:pivotY="12"
                android:rotation="270">
                <path
                    android:pathData="M3,21 C8,18 10,12 10,10 C10,8 12,5 18,3"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round"
                    android:strokeWidth="1.5"
                    android:strokeColor="#A0D4E1D5" />
            </group>
        </vector>
    </item>

</layer-list>